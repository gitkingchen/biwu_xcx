/* pages/choice/choice.wxss */

.title{
	padding:30rpx 30rpx 18rpx;
	font-size:28rpx;
	color:#888;
	width:100%;
	box-sizing:border-box;
}

.content{
	position:relative;
	background-color:#fff;
}

.wrap{
	padding:20rpx 30rpx;
	position:relative;
	border-bottom:1px solid #eee;
}

.wrap view{
	display:inline-block;
}


.operate-btn{
	width:52rpx;
	font-size:32rpx;
	text-align:center;
	border-radius:50%;
	border:2rpx solid #33cd5f;
}